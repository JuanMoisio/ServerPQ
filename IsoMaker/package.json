{
"name": "pq-usb-creator",
"version": "0.1.0",
"private": true,
"type": "module",
"main": "dist-electron/main.js",
"author": "PQS Labs",
"scripts": {
"dev": "run-p dev:renderer dev:main",
"dev:renderer": "vite",
"dev:main": "TS_NODE_TRANSPILE_ONLY=1 ts-node --swc electron/main.ts",
"build": "run-s build:renderer build:main",
"build:renderer": "vite build",
"build:main": "tsc -p tsconfig.json && node scripts/copy-electron.js",
"package": "electron-builder -mwl",
"lint": "eslint . --ext .ts,.tsx,.js",
"format": "prettier --write ."
},
"dependencies": {
"@electron/remote": "^2.1.2",
"etcher-sdk": "^9.0.0",
"react": "^18.3.1",
"react-dom": "^18.3.1"
},
"devDependencies": {
"@types/node": "^22.5.0",
"@types/react": "^18.3.3",
"@types/react-dom": "^18.3.0",
"electron": "^31.3.0",
"electron-builder": "^24.13.3",
"eslint": "^9.9.0",
"eslint-plugin-react": "^7.35.0",
"npm-run-all": "^4.1.5",
"prettier": "^3.3.3",
"ts-node": "^10.9.2",
"typescript": "^5.6.2",
"vite": "^5.4.2"
}
}